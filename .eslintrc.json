{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "camelcase": "off",
      "no-console": "off",
      "class-methods-use-this": "off",
      "no-unused-vars": ["error", { "argsIgnorePattern": "next" }]
    }
}
